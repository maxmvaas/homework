// Нулевая задача:
#include <iostream>

using namespace std;

int main() {
	int n, k;
	cin >> n >> k;
	int* arr = new int[n];
	int* arr2 = new int[k];
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}
	for (int i = 0; i < n; i++) {
		if (arr[i] >= 0 && arr[i] < k) {
			arr[i] = 0;
		}
	}
	for (int i = 0; i < n; i++) {
		cout << arr[i] << " ";
	}
}
// Задача под номером один:
#include <iostream>

using namespace std;

int main() {
	int n;
	double value;
	cin >> n;
	double* arr = new double[n];
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}
	if (n % 2 == 0) {
		value = arr[(n / 2) - 1];
		arr[(n / 2) - 1] = arr[(n / 2)];
		arr[(n / 2)] = value;
	}
	else if (n % 2 != 0) {
		arr[n / 2] *= 2;
	}
	for (int i = 0; i < n; i++) {
		cout << arr[i] << " ";
	}
}
// Задача под номером два:
#include <iostream>

using namespace std;

int main() {
	int n;
	double sum;
	cin >> n;
	double* arr = new double[n];
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}
	for (int i = 1; i < n; i++) {
		if (i % 2 == 1) {
			sum = arr[i] + arr[i - 1];
			arr[i] = sum;
		}
	}
	for (int i = 0; i < n; i++) {
		cout << arr[i] << " ";
	}
}
// Задача под номером три:
#include <iostream>

using namespace std;

int main() {
	int n, k, amount;
	cin >> n >> k;
	amount = n + k;
	int* arr = new int[n];
	int* arr2 = new int[k];
	int* arr3 = new int[amount];
	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}
	for (int i = 0; i < k; i++) {
		cin >> arr2[i];
	}
	for (int i = 0; i < amount; i++) {
		if (i < n) {
			arr3[i] = arr[i];
		}
		else if (i >= k) {
			arr3[i] = arr2[i];
		}
	}
	for (int i = 0; i < amount; i++) {
		cout << arr3[i] << " ";
	}
}
